<table class="table table-striped table-condensed">
  <thead>
  <tr>
    <th>VLAN Number</th>
    <th>VLAN Name</th>
    <th>Static IP Start</th>
    <th>Static IP End</th>
    <th>Subnet Mask</th>
    <th>Gateway</th>
    <th>Description</th>
  </tr>
  </thead>
<tbody>
  <%# count = @results.offset + 1 %>
  <% vlan.each do |v| %>
    <tr>
      <td><%= v.vlan_number %></td>
      <td><%= v.vlan_name %></td>
      <td><%= v.static_ip_start %></td>
      <td><%= v.static_ip_end %></td>
      <td><%= v.subnet_mask %></td>
      <td><%= v.gateway %></td>
      <td><%= v.vlan_description %></td>
    </tr>
  <% end %>
</tbody>
</table>
