<table class="table table-striped table-condensed">
<thead>
  <tr>
    <th>LAN Number</th>
    <th>LAN Name</th>
    <th>LAN Description</th>
  </tr>
</thead>
<tbody>
  <%# count = @results.offset + 1 %>
  <% lan.each do |v| %>
    <tr>
      <td><%= v.lan_number %></td>
      <td><%= v.lan_name %></td>
      <td><%= v.lan_description %></td>
    </tr>
  <% end %>
</tbody>
</table>
