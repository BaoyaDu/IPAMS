<!-- Reserved address list -->
<h4>Reserved Address List</h4> 
<% vlan = uneditable %>

<table class="table table-striped table-condensed">
  <thead>
  <tr>
    <th>No.</th>
    <th>IP Adress</th>
    <th>Description</th>
  </tr>
  </thead>
  <tbody>
  <% count = 0 %>
  <% vlan.reserved_addresses.each do |ra| %>
  <tr>
    <td><%= count += 1 %></td>
    <td><%= ra.ip %></td>
    <td><%= ra.description %></td>
  </tr>
  <% end %>
  </tbody>
</table>
