<tr>
  <% lan = uneditable %>
  <% cache lan do %><%# partial row caching %>
    <td><%= lan.lan_number %></td>
    <td><%= lan.lan_name %></td>
    <td><%= lan.lan_description %></td>
  <% end %>

  <td>
    <% if policy(lan).show? %>
      <%= link_to 'Show', lan, class: 'btn btn-primary btn-xs' %>
    <% end %>
  </td>
 </tr>
